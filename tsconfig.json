{
    "compilerOptions": {
      "alwaysStrict": false,
      "declaration": true,
      "declarationMap": true,
      "downlevelIteration": true,
      "importHelpers": true,
      "inlineSources": true,
      "lib": ["es6", "dom"],
      "moduleResolution": "node",
      "noEmitHelpers": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitUseStrict": true,
      "preserveWatchOutput": true,
      "pretty": true,
      "sourceMap": true,
      "strict": true,
      "strictBindCallApply": false,
      "target": "es5"
    }
  }
  