{
  "name": "stacktrace-parser-node",
  "version": "1.0.3",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "stacktrace",
    "error",
    "trace",
    "stack",
    "node"
  ],
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/piotr-szewczyk/stacktrace-parser-node",
  "repository": {
    "type": "git",
    "url": "git://github.com/piotr-szewczyk/stacktrace-parser-node.git"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json --outDir dist",
    "lint": "run-s lint:prettier",
    "lint:prettier": "prettier */**/*.{js,ts} --write",
    "prebuild": "rimraf ./dist",
    "prepack": "yarn lint",
    "package": "yarn build && npm pack",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "jest": "^26.5.5",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "tslint": "^5.11.0",
    "npm-run-all": "^4.1.2"
  },
  "jest": {
    "verbose": false,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
